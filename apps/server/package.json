{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "start": "node dist/main.js",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint \"src/**/*.{ts,js}\" --max-warnings=0",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d ./data-source.ts ./src/migrations/auto",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ./data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d ./data-source.ts",
    "test": "cross-env NODE_ENV=test jest --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "@nestjs/swagger": "^7.1.15",
    "swagger-ui-express": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "ioredis": "^5.4.1",
    "dotenv": "^16.3.1",
    "mysql2": "^3.9.7",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/bcryptjs": "^2.4.2",
    "eslint": "^8.57.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.0",
    "@types/jest": "^29.5.11",
    "@types/supertest": "^2.0.16",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1"
  }
}
